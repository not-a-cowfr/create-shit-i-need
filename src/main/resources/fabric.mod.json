{
	"schemaVersion": 1,
	"id": "create-fly-addons",
	"version": "${version}",
	"name": "Create Fly Addons",
	"description": "adds addons i want that arent available for create-fly",
	"authors": [
		"not-a-cowfr"
	],
	"contact": {
		"sources": "https://github.com/not-a-cowfr/create-fly-addons"
	},
	"license": "CC0-1.0",
	"icon": "assets/create-fly-addons/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "fr.notacow.create.CreateFlyAddons",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "fr.notacow.create.CreateFlyAddonsClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"create-fly-addons.mixins.json",
		{
			"config": "create-fly-addons.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}